local frameIds = {
    126236556586764,
    136412915020251,
    118280109681409,
    101381777744900,
    102672201310426,
    124200335226270,
    89407138360978,
    111538665898847,
    109147998960875,
    96634424934105,
    121058843996793,
    124085269518949,
    80121979897261,
    105390718040660,
    81239864778767,
    93607458795390,
    129395887027556,
    96412008726713,
    131052168057364
}

local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "NytheruneHubVideo"
screenGui.ResetOnSpawn = false
screenGui.Parent = playerGui

-- Fundo borrado
local background = Instance.new("ImageLabel")
background.Size = UDim2.new(2, 0, 2, 0)
background.Position = UDim2.new(-0.5, 0, -0.5, end
background.Image = "rbxassetid://94067569796788"
background.BackgroundTransparency = 1
background.ScaleType = Enum.ScaleType.Crop
background.Parent = screenGui
background.ZIndex = 0

-- Texto centralizado
local textLabel = Instance.new("TextLabel")
textLabel.Size = UDim2.new(1, 0, 0.2, 0)
textLabel.Position = UDim2.new(0, 0, 0.4, 0)
textLabel.BackgroundTransparency = 1
textLabel.Text = "Nytherune Hub"
textLabel.Font = Enum.Font.BuilderSansBold
textLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
textLabel.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
textLabel.TextStrokeTransparency = 0
textLabel.TextScaled = true
textLabel.Parent = screenGui
textLabel.ZIndex = 10 -- garantir na frente das imagens

-- Audio speed up
local sound = Instance.new("Sound")
sound.SoundId = "rbxassetid://93155576339935"
sound.PlaybackSpeed = 1.5 -- speed up
sound.Parent = screenGui
sound:Play()

-- Executar frames acumulativos
local fps = 4
local interval = 1 / fps

for repeatCount = 1, 2 do
    for i, id in ipairs(frameIds) do
        local frame = Instance.new("ImageLabel")
        frame.Size = UDim2.new(0, 1280, 0, 720)
        frame.AnchorPoint = Vector2.new(0.5, 0.5)
        frame.Position = UDim2.new(0.5, 0, 0.5, 0)
        frame.BackgroundTransparency = 1
        frame.ScaleType = Enum.ScaleType.Fit
        frame.Image = "rbxassetid://" .. id
        frame.Parent = screenGui
        frame.ZIndex = 5
        wait(interval)
    end
end

-- Destruir tudo ao final
screenGui:Destroy()
